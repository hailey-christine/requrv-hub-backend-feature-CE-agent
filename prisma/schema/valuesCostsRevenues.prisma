model ValuesCostsRevenues {
    id     String @id @default(uuid()) @db.Uuid
    debit  Float  @default(0)
    credit Float  @default(0)

    percentage     Float   @default(0)
    isPreviousYear Boolean @default(false)
    balance        Float   @default(0)

    isCalculatedIntoHistorical Boolean @default(false)

    // RELATIONS
    incomeStatement   IncomeStatement @relation(fields: [incomeStatementId], references: [id])
    incomeStatementId String             @db.Uuid

    chartAccount   ChartAccount @relation(fields: [chartAccountId], references: [id])
    chartAccountId String       @db.Uuid

    //AUTOGENERATED
    createdAt DateTime @default(now()) @db.Timestamp()
    updatedAt DateTime @updatedAt @db.Timestamp()
}
